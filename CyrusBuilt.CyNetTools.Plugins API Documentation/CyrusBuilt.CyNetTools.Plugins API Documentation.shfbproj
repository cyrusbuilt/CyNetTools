    <Name>CyrusBuilt.CyNetTools.Plugins_API_Documentation</Name>
    <!-- SHFB properties -->
    <FrameworkVersion>.NET Framework 4.0</FrameworkVersion>
    <OutputPath>Help\</OutputPath>
    <HtmlHelpName>CyrusBuilt.CyNetTools.Plugins_API_Documentation</HtmlHelpName>
    <Language>en-US</Language>
    <DocumentationSources>
      <DocumentationSource sourceFile="..\CyrusBuilt.CyNetTools.Plugins\bin\Debug\CyrusBuilt.CyNetTools.Plugins.XML" />
      <DocumentationSource sourceFile="..\CyrusBuilt.CyNetTools.Plugins\bin\Debug\CyrusBuilt.CyNetTools.Plugins.dll" />
