    <ContentPlacement>AboveNamespaces</ContentPlacement>
    <NamespaceSummaries>
      <NamespaceSummaryItem name="CyrusBuilt.CyNetTools.Plugins" isDocumented="True">Provides a plugin API for CyNetTools.  Contains a collection of interfaces and classes for building plugins for CyNetTools.</NamespaceSummaryItem>
      <NamespaceSummaryItem name="CyrusBuilt.CyNetTools.Plugins.Events" isDocumented="True">Plugin events and handler delegates.</NamespaceSummaryItem>
